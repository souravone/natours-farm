    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.1",
    "leaflet": "^1.9.4",
    "mongodb": "^5.7.0",
    "mongoose": "^7.3.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
